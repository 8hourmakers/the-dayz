{
    "extends": ["airbnb"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "no-unused-vars": 1,
        "no-trailing-spaces": [2, { "skipBlankLines": true }],
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0
    }
}
